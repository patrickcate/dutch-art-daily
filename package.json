{
  "name": "dutch-art-daily",
  "version": "2.2.2",
  "description": "A new Dutch Golden Age artwork delivered every day!",
  "author": "Patrick Cate",
  "private": true,
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie <= 11"
  ],
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "serve": "nuxt",
    "generate": "nuxt generate",
    "generate:modern": "nuxt generate --modern",
    "generate:images": "rm -rf ./static/photos && rm -rf ./data && node -e 'require(\"./utils/generate-photo-data.js\").generatePhotoData()'",
    "analyze": "yarn build --analyze",
    "lint:scripts": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:scripts:fix": "eslint --ext .js,.vue --fix --ignore-path .gitignore . && prettier --list-different --write \"**/*.{js,json,vue,yml}\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"**/*.vue\" \"**/*.scss\"",
    "lint:styles:fix": "stylelint --fix \"**/*.vue\" \"**/*.scss\" && prettier --list-different --write \"**/*.{css,scss,vue}\"",
    "lint:all:fix": "eslint --ext .js,.vue --fix . && stylelint --fix \"**/*.vue\" \"**/*.scss\" && prettier --list-different --write \"**/*.{js,json,css,scss,vue,yml}\"",
    "precommit": "yarn lint:all:fix",
    "gitcommit": "git-cz",
    "release": "standard-version"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "date-fns": "^1.29.0",
    "nuxt": "^2.0.0",
    "swiper": "^5.2.1",
    "vue-screen": "^1.2.0"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.7.7",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/sitemap": "^2.0.1",
    "babel-eslint": "^10.0.2",
    "commitizen": "^3.0.0",
    "core-js": "3",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "2.0.0",
    "cz-customizable": "^6.0.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "font-face-generator": "^1.1.1",
    "imagemin-mozjpeg": "^8.0.0",
    "include-media": "^1.4.9",
    "jimp": "^0.9.3",
    "markdownlint-cli": "^0.17.0",
    "modularscale-sass": "^3.0.8",
    "node-sass": "^4.10.0",
    "node-vibrant": "^3.0.0",
    "nodemon": "^1.11.0",
    "normalize.css": "^8.0.1",
    "nuxt-imagemin": "^1.0.0",
    "postcss-easing-gradients": "^3.0.1",
    "prettier": "^1.18.2",
    "read-data": "^1.1.0",
    "sass-loader": "^7.1.0",
    "standard-version": "^7.0.1",
    "stylelint": "^10.0.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-scss": "^3.3.1",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "write-data": "^1.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  }
}
