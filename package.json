{
  "name": "dutch-art-daily",
  "version": "2.5.1",
  "description": "A new Dutch Golden Age artwork delivered every day!",
  "author": "Patrick Cate",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:e2e": "wait-on http://localhost:3000 && cypress open",
    "build": "nuxt build",
    "start": "nuxt start",
    "serve": "yarn dev",
    "analyze": "yarn build --analyze",
    "generate": "nuxt generate --modern",
    "generate:images": "node -e 'require(\"./utils/generate-photo-data.js\").generatePhotoData()'",
    "reinstall": "rm -rf ./node_modules && yarn install",
    "clean:images": "rm -rf ./static/photos",
    "clean:data": "rm -rf ./data",
    "clean:all": "yarn clean:images && clean:data",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:markdown": "markdownlint --ignore=\"CHANGELOG.md\"",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"**/*.{vue,scss}\"",
    "lint:all:markdown": "yarn lint:markdown \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint": "run-s lint:all:*",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:e2e": "cypress run",
    "test": "run-s test:*",
    "gitcommit": "git-cz",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.2.2",
    "date-fns": "^2.11.0",
    "nuxt": "^2.14.7",
    "swiper": "^6.6.1",
    "vue-screen": "^1.5.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/runtime-corejs3": "^7.12.5",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.0",
    "@nuxtjs/sitemap": "^2.0.1",
    "@vue/test-utils": "^1.1.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.2",
    "core-js": "^3.7.0",
    "cross-env": "^7.0.0",
    "cypress": "^7.3.0",
    "cz-conventional-changelog": "^3.0.0",
    "cz-customizable": "^6.0.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^7.2.2",
    "font-face-generator": "^1.1.1",
    "husky": "^6.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "include-media": "^1.4.9",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "jimp": "^0.16.0",
    "lint-staged": "^11.0.0",
    "markdownlint-cli": "^0.27.0",
    "modularscale-sass": "^3.0.8",
    "node-sass": "^6.0.0",
    "node-vibrant": "^3.1.6",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "nuxt-babel": "^1.0.1",
    "nuxt-imagemin": "^1.0.0",
    "postcss-easing-gradients": "^3.0.1",
    "prettier": "^2.0.0",
    "read-data": "^1.1.0",
    "sass-loader": "^10.0.0",
    "standard-version": "^9.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.3.1",
    "svg-sprite-loader": "^6.0.6",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "vue-jest": "^4.0.0-beta.2",
    "wait-on": "^5.0.5",
    "write-data": "^1.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.0.0"
  }
}
